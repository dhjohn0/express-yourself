{{#extend "iFrame"}}
  {{#append "body"}}
    <form action="{{query.url}}" method="POST" enctype="multipart/form-data">
      <div class="form-group">
        <input type="file" name="file" class="form-control" />
      </div>
      <div class="form-group">
        <input type="submit" class="btn btn-primary" value="Upload" />
      </div>
    </form>
  {{/append}}
{{/extend}}